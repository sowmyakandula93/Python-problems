def check_anagrams(word1_wspace, word2_wspace):
    dict1 = {}
    dict2 = {}
    word1 = word1_wspace.replace(" ","")
    word2 = word2_wspace.replace(" ","")
    for char in word1:
        if char not in dict1 and char.isalnum():
            dict1[char] = 1
        else:
            dict1[char] += 1
    for char in set(word2):
        if not char.isalnum():
            continue
        elif char not in dict1:
            return False
        elif word2.count(char) != dict1[char]:
            return False
    return True        

input_word1 = input("provide first word: ").lower()
input_word2 = input("provide second word: ").lower()
if input_word1 != "" or input_word2 != "":
    if check_anagrams(input_word1, input_word2):
        print("Anagrams")
    else:
        print("Not anagrams")
else:
    print("Please provide non empty strings as input words")
